{
  "name": "Eighty",
  "version": "1.2.0",
  "main": "main.js",
  "devDependencies": {
    "electron": "^9.4",
    "electron-builder": "^20.40.2"
  },
  "scripts": {
    "start": "electron .",
    "pack": "./node_modules/.bin/electron-builder --dir",
    "dist": "./node_modules/.bin/electron-builder"
  },
  "repository": "https://github.com/npezza93/eighty",
  "build": {
    "productName": "Eighty",
    "mac": {
      "extendInfo": {
        "NSRequiresAquaSystemAppearance": false
      },
      "target": [
        {
          "target": "dmg",
          "arch": [
            "x64"
          ]
        },
        {
          "target": "zip",
          "arch": [
            "x64"
          ]
        }
      ]
    },
    "dmg": {
      "icon": "build/icon.icns",
      "iconSize": 130,
      "contents": [
        {
          "x": 110,
          "y": 200,
          "type": "file"
        },
        {
          "x": 400,
          "y": 200,
          "type": "link",
          "path": "/Applications"
        }
      ]
    }
  }
}
